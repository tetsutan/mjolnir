{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": [ "last 2 versions" ],
          "modules": false,
          "loose": true
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-runtime"
  ]
}
