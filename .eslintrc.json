{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "react/forbid-prop-types": 0,
        "no-unused-vars": 0
    }
}
